<mujoco model="WalkON5">
    <compiler inertiafromgeom="true" angle="degree"/>
    <default>
        <joint limited="true" damping="1" armature="0"/>
        <geom condim="1" material="matgeom"/>
    </default>
    <option timestep=".005" iterations="50" tolerance="1e-16" solver="Newton" jacobian="dense" cone="pyramidal">
        <flag contact="enable"/>
    </option>
    <size nconmax="50" njmax="500" nstack="10000"/>
    <visual>
        <map force="0.1" zfar="30"/>
        <rgba haze="0.35 0.35 0.35 1"/>
        <quality shadowsize="2048"/>
        <global offwidth="1200" offheight="960"/>
    </visual>
    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
        <texture name="texgeom" type="cube" builtin="flat" mark="cross" width="127" height="1278"
            rgb1="0.8 0.8 0.8" rgb2="0.6 0.6 0.6" markrgb="1 1 1" random="0.01"/>
        <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="1 1" texuniform="true"/>
        <material name="matgeom" texture="texgeom" texuniform="true" rgba="0.8 0.8 0.8 1"/>
        <mesh name="BASE" file="../walkon_stls/BASE.STL"/>
        <mesh name="L_BASE" file="../walkon_stls/L_BASE.STL"/>
        <mesh name="L_HIP" file="../walkon_stls/L_HIP.STL"/>
        <mesh name="L_THIGH" file="../walkon_stls/L_THIGH.STL"/>
        <mesh name="L_SHANK" file="../walkon_stls/L_SHANK.STL"/>
        <mesh name="L_ANKLE" file="../walkon_stls/L_ANKLE.STL"/>
        <mesh name="L_FOOT" file="../walkon_stls/L_FOOT.STL"/>
        <mesh name="R_BASE" file="../walkon_stls/R_BASE.STL"/>
        <mesh name="R_HIP" file="../walkon_stls/R_HIP.STL"/>
        <mesh name="R_THIGH" file="../walkon_stls/R_THIGH.STL"/>
        <mesh name="R_SHANK" file="../walkon_stls/R_SHANK.STL"/>
        <mesh name="R_ANKLE" file="../walkon_stls/R_ANKLE.STL"/>
        <mesh name="R_FOOT" file="../walkon_stls/R_FOOT.STL"/>
    </asset>
    <worldbody>
        <geom name="floor" pos="0 0 0" size="0 0 .25" type="plane" material="matplane" condim="3"/>
        <light name="light" pos="0 5 5"/>

        <body name="BASE" pos="0 0 1.2">
            <freejoint/>
            <geom type="mesh" mesh="BASE" pos="0 0 0"/>
            <inertial mass="4.06288" pos="0.08148 0.14026 0.0055"/>
            <!-- LEFT LIMB -->
            <body name="L_BASE" pos="0 .105 0">
                <joint name="JOINT_LH_ABD" pos="0 0 0" axis="1 0 0" range="-10 15"/>
                <geom type="mesh" mesh="L_BASE"/>
                <inertial mass="4.90278" pos="0.02442 0.38146 -0.0482"/>
                <body name="L_HIP" pos="-.2125 .155 -.04085">
                    <joint name="JOINT_LH_ROT" pos="0 0 0" axis="0 0 1" range="-15 15" damping="5.65"/>
                    <geom type="mesh" mesh="L_HIP"/>
                    <inertial mass="4.36022" pos="-0.12588 0.41316 -0.20155"/>
                    <body name="L_THIGH" pos="0 .00175 -.15415">
                        <joint name="JOINT_LH_EXT" axis="0 1 0" range="-115 30" damping="3.25"/>
                        <geom type="mesh" mesh="L_THIGH"/>
                        <inertial mass="2.70534" pos="-0.13199 0.4022 -0.2723"/>
                        <body name="L_SHANK" pos="-.009 -.025 -.43062">
                            <joint name="JOINT_LK_EXT" axis="0 1 0" range="5 125" damping="3"/>
                            <geom type="mesh" mesh="L_SHANK"/>
                            <inertial mass="2.49032" pos="-0.01306 0.29591 -0.01005"/>
                            <body name="L_ANKLE" pos="0.04227 -0.11025 -0.38744" euler="0 -45 0">
                                <joint name="JOINT_LA_INV" axis="1 0 0" range="-30 30"/>
                                <geom type="mesh" mesh="L_ANKLE"/>
                                <inertial mass="0.55947" pos="-0.01557 0.27552 -0.01969"/>
                                <body name="L_FOOT" pos="0 0 0" euler="0 45 0">
                                    <joint name="JOINT_LA_PLA" pos="0 0 0" axis="0 1 0" range="-30 115"/>
                                    <geom type="mesh" mesh="L_FOOT"/>
                                    <inertial mass="7.55104" pos="0.04099 0.27747 0.01767"/>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>

            <!-- RIGHT LIMB -->
            <body name="R_BASE" pos="0 -.105 0">
                <joint name="JOINT_RH_ABD" pos="0 0 0" axis="1 0 0" range="-15 10"/>
                <geom type="mesh" mesh="R_BASE"/>
                <inertial mass="4.90278" pos="0.02442 -0.10094 -0.0482"/>
                <body name="R_HIP" pos="-.2125 -.155 -.04085">
                    <joint name="JOINT_RH_ROT" pos="0 0 0" axis="0 0 1" range="-15 15" damping="5.65"/>
                    <geom type="mesh" mesh="R_HIP"/>
                    <inertial mass="4.36022" pos="-0.12588 -0.13264 -0.20155"/>
                    <body name="R_THIGH" pos="0 -.00175 -.15415">
                        <joint name="JOINT_RH_EXT" axis="0 1 0" range="-115 30" damping="3.25"/>
                        <geom type="mesh" mesh="R_THIGH"/>
                        <inertial mass="2.70534" pos="-0.13199 -0.12168 -0.2723"/>
                        <body name="R_SHANK" pos="-.009 .025 -.43062">
                            <joint name="JOINT_RK_EXT" axis="0 1 0" range="5 125" damping="3"/>
                            <geom type="mesh" mesh="R_SHANK"/>
                            <inertial mass="2.49032" pos="-0.01306 -0.01539 -0.01005"/>
                            <body name="R_ANKLE" pos="0.04227 0.11025 -0.38744" euler="0 -45 0">
                                <joint name="JOINT_RA_INV" axis="1 0 0" range="-30 30"/>
                                <geom type="mesh" mesh="R_ANKLE"/>
                                <inertial mass="0.55947" pos="-0.01557 0.005 -0.01969"/>
                                <body name="R_FOOT" pos="0 0 0" euler="0 45 0">
                                    <joint name="JOINT_RA_PLA" pos="0 0 0" axis="0 1 0" range="-30 115"/>
                                    <geom type="mesh" mesh="R_FOOT"/>
                                    <inertial mass="7.55104" pos="0.04099 0.00305 0.01767"/>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <!-- <actuator> -->
        <!-- LEFT LIMB ACTUATORS -->
        <!-- <general name="ACT_L_HIP_ABD" joint="JOINT_LH_ABD" gear="1" ctrllimited="true" ctrlrange="-250 250"/>
        <general name="ACT_L_HIP_ROT" joint="JOINT_LH_ROT" gear="1" ctrllimited="true" ctrlrange="-135 135"/>
        <general name="ACT_L_HIP_EXT" joint="JOINT_LH_EXT" gear="1" ctrllimited="true" ctrlrange="-200 200"/>
        <general name="ACT_L_KNEE_EXT" joint="JOINT_LK_EXT" gear="1" ctrllimited="true" ctrlrange="-200 200"/>
        <general name="ACT_L_ANK_INVER" joint="JOINT_LA_INV" gear="1" ctrllimited="true" ctrlrange="-70 70"/>
        <general name="ACT_L_ANK_PLANTAR" joint="JOINT_LA_PLA" gear="1" ctrllimited="true" ctrlrange="-170 170"/> -->

        <!-- RIGHT LIMB ACTUATORS -->
        <!-- <general name="ACT_R_HIP_ABD" joint="JOINT_RH_ABD" gear="1" ctrllimited="true" ctrlrange="-250 250"/>
        <general name="ACT_R_HIP_ROT" joint="JOINT_RH_ROT" gear="1" ctrllimited="true" ctrlrange="-135 135"/>
        <general name="ACT_R_HIP_EXT" joint="JOINT_RH_EXT" gear="1" ctrllimited="true" ctrlrange="-200 200"/>
        <general name="ACT_R_KNEE_EXT" joint="JOINT_RK_EXT" gear="1" ctrllimited="true" ctrlrange="-200 200"/>
        <general name="ACT_R_ANK_INVER" joint="JOINT_RA_INV" gear="1" ctrllimited="true" ctrlrange="-70 70"/>
        <general name="ACT_R_ANK_PLANTAR" joint="JOINT_RA_PLA" gear="1" ctrllimited="true" ctrlrange="-170 170"/> -->
    <!-- </actuator> -->

    <actuator>
        <!-- LEFT LIMB ACTUATORS -->
        <general name="ACT_L_HIP_ABD" joint="JOINT_LH_ABD" gear="1" ctrllimited="false"/>
        <general name="ACT_L_HIP_ROT" joint="JOINT_LH_ROT" gear="1" ctrllimited="false"/>
        <general name="ACT_L_HIP_EXT" joint="JOINT_LH_EXT" gear="1" ctrllimited="false"/>
        <general name="ACT_L_KNEE_EXT" joint="JOINT_LK_EXT" gear="1" ctrllimited="false"/>
        <general name="ACT_L_ANK_INVER" joint="JOINT_LA_INV" gear="1" ctrllimited="false"/>
        <general name="ACT_L_ANK_PLANTAR" joint="JOINT_LA_PLA" gear="1" ctrllimited="false"/>

        <!-- RIGHT LIMB ACTUATORS -->
        <general name="ACT_R_HIP_ABD" joint="JOINT_RH_ABD" gear="1" ctrllimited="false"/>
        <general name="ACT_R_HIP_ROT" joint="JOINT_RH_ROT" gear="1" ctrllimited="false"/>
        <general name="ACT_R_HIP_EXT" joint="JOINT_RH_EXT" gear="1" ctrllimited="false"/>
        <general name="ACT_R_KNEE_EXT" joint="JOINT_RK_EXT" gear="1" ctrllimited="false"/>
        <general name="ACT_R_ANK_INVER" joint="JOINT_RA_INV" gear="1" ctrllimited="false"/>
        <general name="ACT_R_ANK_PLANTAR" joint="JOINT_RA_PLA" gear="1" ctrllimited="false"/>
    </actuator>

</mujoco>

